meta {
  name: Alle Seite 9999
  type: http
  seq: 3
}

get {
  url: {{restUrl}}?page=9999
  body: none
  auth: inherit
}

params:query {
  page: 9999
}

headers {
  Accept: application/json
}

assert {
  res.status: eq 404
  res.headers['content-type']: matches ^application\/json(; charset=utf-8)?$
  res.body.statusCode: eq 404
  res.body.error: eq Not Found
}

settings {
  encodeUrl: true
  timeout: 0
}
