meta {
  name: nest
}

auth {
  mode: none
}

vars:pre-request {
  baseUrl: https://localhost:3000
  keycloak_url: http://localhost:8880
  realm: nest
  realm_url: {{keycloak_url}}/realms/{{realm}}
  authUrl: {{baseUrl}}/auth
  tokenUrl: {{authUrl}}/token
  oidc_token_url: {{realm_url}}/protocol/openid-connect/token
  oidc_auth_url: {{realm_url}}/protocol/openid-connect/token
}
